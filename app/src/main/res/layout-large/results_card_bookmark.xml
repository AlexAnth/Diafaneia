<?xml version="1.0" encoding="utf-8"?>

<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    android:id="@+id/cv"
    card_view:cardPreventCornerOverlap="false">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="#ffffff"
        android:weightSum="1"
        android:paddingTop="6dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/background_with_shadow"
            android:layout_margin="2dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="#8191B2">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="ΑΔΑ :"
                    android:id="@+id/textView22"
                    android:textColor="#ffffff"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="7dp"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp" />

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="20dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="112"
                    android:id="@+id/protoc_num"
                    android:textColor="#ffffff"
                    android:layout_alignTop="@+id/textView23"
                    android:layout_toRightOf="@+id/textView23"
                    android:layout_toEndOf="@+id/textView23"
                    android:layout_marginLeft="7dp"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="03-0538"
                    android:id="@+id/ADA"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="5dp"
                    android:layout_alignTop="@+id/textView22"
                    android:layout_toRightOf="@+id/textView22"
                    android:layout_toEndOf="@+id/textView22"
                    android:layout_marginStart="5dp"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Αρ. Πρ :"
                    android:id="@+id/textView23"
                    android:textColor="#ffffff"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="7dp"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp"/>

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:id="@+id/download_btn"
                    android:src="@drawable/bookmark_icon"
                    android:paddingTop="2dp"
                    android:paddingRight="5dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="12/7/2016"
                    android:id="@+id/date"
                    android:textColor="#ffffff"
                    android:layout_marginRight="9dp"
                    android:layout_marginEnd="9dp"
                    android:fontFamily="sans-serif-light"
                    android:layout_alignBaseline="@+id/textView24"
                    android:layout_alignBottom="@+id/textView24"
                    android:layout_toLeftOf="@+id/download_btn"
                    android:layout_toStartOf="@+id/download_btn"
                    android:textSize="18sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Ανάρτηση : "
                    android:id="@+id/textView24"
                    android:textColor="#ffffff"
                    android:fontFamily="sans-serif-light"
                    android:layout_above="@+id/protoc_num"
                    android:layout_toLeftOf="@+id/date"
                    android:layout_toStartOf="@+id/date"
                    android:textSize="18sp"
                    android:paddingTop="3dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="55dp"
                android:background="#566281"
                android:id="@+id/relativeLayout10">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Διεύθυνση Διαρκών Επιτροπών, Λοιπές πράξεις"
                    android:id="@+id/Sectors"
                    android:textColor="#A8D1E4"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp"
                    android:paddingBottom="2dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relativeLayout10"
                android:paddingBottom="4dp"
                android:id="@+id/relativeLayout11"
                android:layout_marginBottom="4dp"
                android:layout_alignParentEnd="false"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_alignParentRight="false">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Medium Text"
                    android:id="@+id/textView21"
                    android:layout_above="@+id/imageView9"
                    android:layout_alignTop="@+id/imageView8"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="3dp"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView8"
                    android:src="@drawable/boolet_blue"
                    android:layout_marginTop="32dp"
                    android:paddingTop="10dp"
                    android:layout_below="@+id/imageView7"
                    android:layout_toLeftOf="@+id/text_bull_1"
                    android:layout_toStartOf="@+id/text_bull_1" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView7"
                    android:src="@drawable/boolet_blue"
                    android:layout_below="@+id/relativeLayout10"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="4dp"
                    android:paddingTop="2dp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView9"
                    android:src="@drawable/boolet_blue"
                    android:layout_alignLeft="@+id/imageView8"
                    android:layout_alignStart="@+id/imageView8"
                    android:layout_marginTop="102dp"
                    android:paddingTop="7dp"
                    android:layout_alignRight="@+id/imageView8"
                    android:layout_alignEnd="@+id/imageView8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Medium Text"
                    android:id="@+id/text_bull_1"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/imageView7"
                    android:layout_above="@+id/textView21"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:textColor="#4D4D4D"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp"
                    android:layout_height="wrap_content"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="γ) ο προϋπολογισμός εξόδων, ο ισολογισμός − απολογισμός της Βουλής των Ελλήνων, μετά την έγκριση τους από την Ολομέλεια της Βουλής, καθώς και τριμηνιαίοι πίνακες εκτέλεσης του προϋπολογισμού της Βουλής, και πίνακες στοχοθεσίας και επίτευξης ή απόκλισης στ "
                    android:id="@+id/text_bull_2"
                    android:textColor="#4D4D4D"
                    android:layout_below="@+id/text_bull_1"
                    android:layout_alignLeft="@+id/text_bull_1"
                    android:layout_alignStart="@+id/text_bull_1"
                    android:layout_alignRight="@+id/text_bull_3"
                    android:layout_alignEnd="@+id/text_bull_3"
                    android:layout_above="@+id/text_bull_3"
                    android:layout_marginBottom="4dp"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:layout_marginTop="4dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="ΕΥΑΓΓΕΛΟΣ - ΒΑΣΙΛΕΙΟΣ I. ΜΕΪΜΑΡΑΚΗΣ, Ο ΠΡΟΕΔΡΟΣ ΤΗΣ ΒΟΥΛΗΣ ΤΩΝ ΕΛΛΗΝΩΝ"
                    android:id="@+id/text_bull_3"
                    android:layout_toRightOf="@+id/imageView9"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:textColor="#4D4D4D"
                    android:layout_alignTop="@+id/imageView9"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:fontFamily="sans-serif-light"
                    android:textSize="18sp"/>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>


</android.support.v7.widget.CardView>